{
  "expo": {
    "name": "Andi",
    "slug": "andi-referral-agent",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "andi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "useHash": true
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "fonts": [  
      "./assets/fonts/DancingScript-Bold.ttf",
      "./assets/fonts/Inter_28pt-Regular.ttf",
      "./assets/fonts/Inter_28pt-Medium.ttf",
      "./assets/fonts/Inter_28pt-SemiBold.ttf",
      "./assets/fonts/Inter_28pt-Bold.ttf"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
